{
  "info": {
    "name": "Place API (Form-Data)",
    "_postman_id": "ducksrow-formdata-collection",
    "description": "Collection for testing Place API endpoints with form-data",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Create Place (Form-Data)",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Authorization",
            "value": "Bearer {{token}}",
            "type": "text"
          }
        ],
        "body": {
          "mode": "formdata",
          "formdata": [
            {
              "key": "name",
              "value": "Cafe Mocha",
              "type": "text"
            },
            {
              "key": "description",
              "value": "A cozy coffee shop with free WiFi and pastries",
              "type": "text"
            },
            {
              "key": "priceLevel",
              "value": "MEDIUM",
              "type": "text"
            },
            {
              "key": "avgPriceMin",
              "value": "50",
              "type": "text"
            },
            {
              "key": "avgPriceMax",
              "value": "120",
              "type": "text"
            },
            {
              "key": "phone",
              "value": "+201234567890",
              "type": "text"
            },
            {
              "key": "websiteUrl",
              "value": "https://cafemocha.com",
              "type": "text"
            },
            {
              "key": "instagramHandle",
              "value": "@cafemocha",
              "type": "text"
            },
            {
              "key": "addressLine",
              "value": "123 Nile Street",
              "type": "text"
            },
            {
              "key": "city",
              "value": "Cairo",
              "type": "text"
            },
            {
              "key": "country",
              "value": "EG",
              "type": "text"
            },
            {
              "key": "geopoint",
              "value": "{\"lat\":30.0444,\"lng\":31.2357}",
              "type": "text"
            },
            {
              "key": "amenities",
              "value": "{\"wifi\":true,\"outdoor\":true,\"parking\":false}",
              "type": "text"
            },
            {
              "key": "tags",
              "value": "[\"coffee\",\"breakfast\",\"chill\"]",
              "type": "text"
            },
            {
              "key": "photos",
              "type": "file",
              "src": "/path/to/photo1.jpg"
            },
            {
              "key": "photos",
              "type": "file",
              "src": "/path/to/photo2.jpg"
            }
          ]
        },
        "url": {
          "raw": "http://localhost:5000/api/places",
          "protocol": "http",
          "host": ["localhost"],
          "port": "5000",
          "path": ["api", "places"]
        }
      },
      "response": []
    }
  ],
  "variable": [
    {
      "key": "token",
      "value": ""
    }
  ]
}
